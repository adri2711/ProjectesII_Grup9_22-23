    {
      "keyword": "symmetry",
      "sentenceType": 0,
      "writingStyle": 1,
      "alreadyInText": false,

      "textPart": {
        "id": "sen4",
        "colorHex": "#000000",
        "flags": 2048,
        "text": "Be it Geometry, Time, logical structures, molecules or language, there is nothing unable to be touched by its Grace. ",
        "font": "bluescreen"
      }
    },
    {
      "keyword": "symmetry",
      "sentenceType": 0,
      "writingStyle": 1,
      "alreadyInText": false,

      "textPart": {
        "id": "sen5",
        "colorHex": "#000000",
        "flags": 2048,
        "text": "Symmetry is the expression of a permanent essence, and thus the proof of the entwinement of the supranatural plane with mundane phenomena. ",
        "font": "bluescreen"
      }
    },
    {
      "keyword": "symmetry",
      "sentenceType": 0,
      "writingStyle": 0,
      "alreadyInText": false,

      "textPart": {
        "id": "sen6",
        "colorHex": "#000000",
        "flags": 2048,
        "text": "Search for its signs. ",
        "font": "bluescreen"
      }
    },
