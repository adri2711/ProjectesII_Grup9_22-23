      "keyword": "symmetry",
      "sentenceType": 0,
      "writingStyle": 0,
      "alreadyInText": false,

      "textPart": {
        "id": "sen6",
        "colorHex": "#000000",
        "flags": 2048,
        "text": "Feel the ecstasy of Perfection. ",
        "font": "bluescreen"
      }
    },
